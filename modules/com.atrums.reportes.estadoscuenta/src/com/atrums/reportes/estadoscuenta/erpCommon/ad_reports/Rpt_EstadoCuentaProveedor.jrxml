	</parameter>
	<parameter name="AUX2_Bpartner" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[($P{Bpartner}==null || $P{Bpartner}.equals("")) ? "" : " and p.c_bpartner_id = '" + $P{Bpartner} + "'"]]></defaultValueExpression>
