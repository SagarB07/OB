	<queryString>
		<![CDATA[SELECT
CE.NO_CONTRATO_EMPLEADO_ID AS IDCONTRATO,
EM.TAXID AS CI,
EM.NAME AS COLABORADOR,
AR.nombre AS AREA,
TCON.NAME AS TIPO_CONTRATO,
CE.DOCUMENTNO AS NUMERO_CONTRATO,
CE.FECHA_INICIO AS FECHA_INICIO,
CE.FECHA_FIN AS FECHA_FIN,
NO_CONTEO_REGISTRO_VAC (CE.NO_CONTRATO_EMPLEADO_ID, 'BR') AS PREPLAN_LIN,
NO_SUMA_REGISTRO_VAC (CE.NO_CONTRATO_EMPLEADO_ID, 'BR') AS PREPLAN_SUMA,
NO_CONTEO_REGISTRO_VAC (CE.NO_CONTRATO_EMPLEADO_ID, 'AP') AS APRPLAN_LIN,
NO_SUMA_REGISTRO_VAC (CE.NO_CONTRATO_EMPLEADO_ID, 'AP') AS APRPLAN_SUMA,
NO_CONTEO_REGISTRO_VAC (CE.NO_CONTRATO_EMPLEADO_ID, 'PA') AS PAPRPLAN_LIN,
NO_SUMA_REGISTRO_VAC (CE.NO_CONTRATO_EMPLEADO_ID, 'PA') AS PAPRLAN_SUMA,
CE.EM_NE_VACACION_PROP AS DIAS_GANADOS,
CE.EM_NE_VACACION_TOM AS DIAS_TOMADOS,
CE.EM_NE_VACACION_PROP - CE.EM_NE_VACACION_TOM AS VAC_RESTANTES
FROM C_BPARTNER EM,
NO_VACACION VAC,
NO_CONTRATO_EMPLEADO CE,
NO_AREA_EMPRESA AR,
C_DOCTYPE TCON
WHERE
VAC.NO_CONTRATO_EMPLEADO_ID = CE.NO_CONTRATO_EMPLEADO_ID
AND CE.C_DOCTYPE_ID = TCON.C_DOCTYPE_ID
AND EM.C_BPARTNER_ID = CE.C_BPARTNER_ID
AND AR.NO_AREA_EMPRESA_ID = CE.em_ne_area_empresa_id
AND CE.DOCSTATUS = 'CO'
GROUP BY CI,COLABORADOR,AREA,
NUMERO_CONTRATO, FECHA_INICIO, FECHA_FIN,
PREPLAN_LIN, PREPLAN_SUMA,APRPLAN_LIN , APRPLAN_SUMA,
DIAS_GANADOS,DIAS_TOMADOS, VAC_RESTANTES,TCON.NAME,ce.no_contrato_empleado_id, IDCONTRATO]]>
	</queryString>
