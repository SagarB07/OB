	<parameter name="DOCUMENT_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
emp.name as NOMBRE,
emp.taxid AS CEDULA,
DOC.NAME,
cemp.em_ne_region AS REGIONID,
(CASE  WHEN em_ne_region = '1' THEN 'SIERRA' WHEN em_ne_region = '2' THEN 'COSTA' ELSE 'NE' END) AS REGIONNAME,
REG.NAME AS PROVINCIA,
LOC.CITY,
to_char(CEMP.FECHA_INICIO , 'yyyy-mm-dd') AS ILABORES,
to_char(CEMP.FECHA_FIN , 'yyyy-mm-dd') AS FLABORES,
CAR.NAME AS CARGO,
CEMP.SALARIO AS SUELDO,
no_obtiene_valor_prov(EMP.C_BPARTNER_ID, 'B1E1B09BFBD54A99A8E1E6FF2A91DB9D',2 ,CEMP.FECHA_FIN ,cemp.no_contrato_empleado_id ) AS DECIMOTERCERO,
no_obtiene_valor_prov(EMP.C_BPARTNER_ID, '73986DBC43E34F25B67766C6409E213F',1 ,CEMP.FECHA_FIN ,cemp.no_contrato_empleado_id ) AS DECIMOCUARTO
FROM c_bpartner emp
left  JOIN no_contrato_empleado cemp  ON cemp.c_bpartner_id = emp.c_bpartner_id
left  JOIN c_bpartner_location LEMP  ON LEMP.c_bpartner_id = emp.c_bpartner_id
left  JOIN C_LOCATION LOC  ON  LOC.C_LOCATION_ID = LEMP.C_LOCATION_ID
left  JOIN C_REGION REG  ON  REG.C_REGION_ID = LOC.C_REGION_ID
left  JOIN C_DOCTYPE DOC  ON  CEMP.C_DOCTYPE_ID = DOC.C_DOCTYPE_ID
left  JOIN ATNORH_CARGO CAR  ON  CAR.ATNORH_CARGO_ID = cemp.EM_ATNORH_CARGO_ID
WHERE
emp.isemployee =  'Y'
AND cemp.isactive = 'Y'
and emp.taxid = '0105424725'
and cemp.no_contrato_empleado_id = '961963FFCF354EA4B822177E523B3000';]]>
	</queryString>
