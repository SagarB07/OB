<?xml version="1.0"?>
  <database name="TRIGGER ASV_DIF_TRG">
    <trigger name="ASV_DIF_TRG" table="ASV_PEDIDO_DETALLE" fires="before" insert="true" update="true" delete="false" foreach="row">
      <body><![CDATA[

BEGIN
    :NEW.diferencia:=:NEW.enviado-:NEW.recibido;
    RETURN NEW;
END ASV_DIF_TRG
]]></body>
    </trigger>
  </database>
