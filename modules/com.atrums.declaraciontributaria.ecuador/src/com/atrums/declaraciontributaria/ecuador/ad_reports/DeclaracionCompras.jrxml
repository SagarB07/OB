	<queryString>
		<![CDATA[select
(select org.name from ad_org org where org.ad_org_id = i.ad_org_id) as sucursal,
i.dateinvoiced,
(select name from c_doctype_trl where c_doctype_id = i.c_doctypetarget_id and ad_language = 'es_EC') as documento,
coalesce(i.em_co_nro_estab, ' ') || '-' || coalesce(i.em_co_punto_emision, ' ') || '-' || coalesce(i.documentno, ' ') as documentno,
coalesce(p.name2, p.name) as name,
coalesce(i.description,'') as description,

case when dt.docbasetype = 'APC' then ats_calc_base_imponible(i.c_invoice_id, '3')*-1 else  ats_calc_base_imponible(i.c_invoice_id, '3') end AS base12,

case when dt.docbasetype = 'APC' then (ats_calc_base_imponible(i.c_invoice_id, '1') + ats_calc_base_imponible(i.c_invoice_id, '2'))*-1 else ats_calc_base_imponible(i.c_invoice_id, '1') + ats_calc_base_imponible(i.c_invoice_id, '2') end AS base0,

case when dt.docbasetype = 'APC' then ats_calc_monto_iva_ice(i.c_invoice_id, 'N')*-1 else ats_calc_monto_iva_ice(i.c_invoice_id, 'N') end AS iva,
case when dt.docbasetype = 'APC' then i.grandtotal*-1 else i.grandtotal end as grandtotal,
coalesce(rc.documentno,'-') as no_retencion,
coalesce(ats_val_retencion_fte(i.c_invoice_id, 1),'-') as codigo,
coalesce(ats_val_retencion_fte(i.c_invoice_id, 2),'0.00') as base_imp_retencion,
coalesce(ats_val_retencion_fte(i.c_invoice_id, 3),'0.00') as valor_retencion,
ats_val_retencion_iva(i.c_invoice_id, 'RB') as ret_iva30,
ats_val_retencion_iva(i.c_invoice_id, 'RS') as ret_iva70,
ats_val_retencion_iva(i.c_invoice_id, 'RC') as ret_iva100

from c_invoice i left join c_bpartner p on i.c_bpartner_id = p.c_bpartner_id
                left join c_doctype dt on i.c_doctype_id = dt.c_doctype_id
                left join ad_org o on i.ad_org_id = o.ad_org_id
                left join co_retencion_compra rc on rc.c_invoice_id = i.c_invoice_id and rc.docstatus <> 'AN'
where i.c_invoice_id IN (
                SELECT C_INVOICE_ID
                  FROM C_INVOICE
                 WHERE PROCESSED = 'Y'
                   AND date(DATEINVOICED)+0 >= $P{DateFrom}
                   AND date(DATEINVOICED)+0 <= $P{DateTo}
                   AND AD_CLIENT_ID IN ($P!{USER_CLIENT})
                   AND ISSOTRX = 'N'
	         and docstatus <> 'VO'
                   and c_doctypetarget_id NOT IN (select c_doctype_id from c_doctype where em_co_tp_comp_autorizador_sri = '0'))
order by 2,1]]>
	</queryString>
