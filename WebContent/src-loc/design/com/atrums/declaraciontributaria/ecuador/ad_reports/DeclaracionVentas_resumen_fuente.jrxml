	<property name="ireport.zoom" value="3.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="DateFrom" class="java.util.Date"/>
	<parameter name="DateTo" class="java.util.Date"/>
	<queryString>
		<![CDATA[select
tr.tiporetencionfuente as codigo,
sum(rvl.valor_retencion) as retencion,
sum(rvl.base_imp_retencion) as baseimp
from c_invoice i
    left join co_retencion_venta rv on (i.c_invoice_id = rv.c_invoice_id and date(rv.dateacct)+0 >= date($P{DateFrom}) and date(rv.dateacct)+0 <= date($P{DateTo}))
    left join co_retencion_venta_linea rvl on rv.co_retencion_venta_id = rvl.co_retencion_venta_id and rvl.tipo = 'FUENTE'
    left join co_bp_retencion_venta brv on rvl.co_bp_retencion_venta_id = brv.co_bp_retencion_venta_id
    left join co_tipo_retencion tr on brv.co_tipo_retencion_id = tr.co_tipo_retencion_id
where i.issotrx = 'Y' AND i.isactive = 'Y' AND i.processed = 'Y' and i.docstatus <> 'VO'
  and date(rv.dateacct)+0 >= date($P{DateFrom})
  and date(rv.dateacct)+0 <= date($P{DateTo})
  and i.c_doctypetarget_id NOT IN (select c_doctype_id from c_doctype where em_co_tp_comp_autorizador_sri = '0')
group by 1
order by 1]]>
	</queryString>
