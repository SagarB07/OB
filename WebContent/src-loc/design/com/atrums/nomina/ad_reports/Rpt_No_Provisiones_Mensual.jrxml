	<parameter name="c_period_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select distinct(c.name) as empleado ,ad.name as agencia, em.nombre as departamento,c.taxid as cedula, dt.name as tipo_documento,
        ce.documentno as contrato, ce.fecha_inicio as fecha_inicio, ce.fecha_fin as fecha_fin, coalesce(ce.salario,0) as salario,
        coalesce(rp.total_ingreso,0) as total_ingreso, coalesce(rp.total_egreso,0) as total_egreso,
        coalesce(rp.total_neto,0) as total_neto, rp.dateacct as fecha_contable,
        (case when em.no_area_empresa_id in ('8C2EDD6274844BEC802EF1721307190D', '0ABF48A38B3748EEBEC6D4B05CB8EC20') then
              'Administrativo'
         else
              'Ventas'
         end) as area_emp,
        coalesce((select coalesce(cd.valor,0)
           from no_re_comision rc, no_re_comision_detalle cd
          where rc.no_re_comision_id = cd.no_re_comision_id
            and rc.c_period_id = rp.c_period_id
            and cd.c_bpartner_id = rp.c_bpartner_id),0) as comision,
        coalesce((select coalesce(sum(round(((ce.salario/240::numeric)*(((100+ch.porcentaje)/100)::numeric))*he.cantida_hora,2)),0)
           from no_registro_hora_extra he
           left join no_config_hora_extra ch on (ch.no_config_hora_extra_id = he.no_config_hora_extra_id)
          where he.c_bpartner_id = rp.c_bpartner_id
            and he.c_period_id = rp.c_period_id),0) as horas_extra,
        coalesce(pl.valor,0) as fondos_reserva,
        coalesce((select coalesce(plm.valor,0)
           from no_rol_pago_provision rpp, no_rol_pago_provision_line ppl, no_rol_provision_line_mes plm
          where rpp.c_bpartner_id = rp.c_bpartner_id
            and rpp.ispago = 'N'
            and rpp.no_rol_pago_provision_id = ppl.no_rol_pago_provision_id
            and ppl.no_tipo_ingreso_egreso_id in ('419716ABE5AE414E8577B3C26C6F823D','AC79B4EB2F5546B2895C290C21F53931')
            and ppl.no_rol_pago_provision_line_id = plm.no_rol_pago_provision_line_id
            and plm.c_period_id = rp.c_period_id),0) as aporte_patronal,
         coalesce((select coalesce(plm.valor,0)
           from no_rol_pago_provision rpp, no_rol_pago_provision_line ppl, no_rol_provision_line_mes plm
          where rpp.c_bpartner_id = rp.c_bpartner_id
            and rpp.ispago = 'N'
            and rpp.no_rol_pago_provision_id = ppl.no_rol_pago_provision_id
            and ppl.no_tipo_ingreso_egreso_id in ('B1E1B09BFBD54A99A8E1E6FF2A91DB9D')
            and ppl.no_rol_pago_provision_line_id = plm.no_rol_pago_provision_line_id
            and plm.c_period_id = rp.c_period_id),0) as decimo_tercero,
        coalesce((select coalesce(plm.valor,0)
           from no_rol_pago_provision rpp, no_rol_pago_provision_line ppl, no_rol_provision_line_mes plm
          where rpp.c_bpartner_id = rp.c_bpartner_id
            and rpp.ispago = 'N'
            and rpp.no_rol_pago_provision_id = ppl.no_rol_pago_provision_id
            and ppl.no_tipo_ingreso_egreso_id in ('73986DBC43E34F25B67766C6409E213F')
            and ppl.no_rol_pago_provision_line_id = plm.no_rol_pago_provision_line_id
            and plm.c_period_id = rp.c_period_id),0) as decimo_cuarto,
         coalesce((select coalesce(plm.valor,0)
           from no_rol_pago_provision rpp, no_rol_pago_provision_line ppl, no_rol_provision_line_mes plm
          where rpp.c_bpartner_id = rp.c_bpartner_id
            and rpp.ispago = 'N'
            and rpp.no_rol_pago_provision_id = ppl.no_rol_pago_provision_id
            and ppl.no_tipo_ingreso_egreso_id in ('DEFEED633A084DAAB2ED2FB2F8BE28A0')
            and ppl.no_rol_pago_provision_line_id = plm.no_rol_pago_provision_line_id
            and plm.c_period_id = rp.c_period_id),0) as vacaciones,
      (select upper('Mes del ' || to_char(per.startdate,'DD') || ' de ' || to_char(per.startdate,'TMMonth') || ' al ' || to_char(per.enddate,'DD') || ' de ' || to_char(per.startdate,'TMMonth') || ' de ' || to_char(per.enddate,'YYYY')) from c_period per where per.c_period_id = rp.c_period_id) as fecha_periodo
  from no_rol_pago_provision rp
  left join c_bpartner c on (c.c_bpartner_id = rp.c_bpartner_id)
  left join no_contrato_empleado ce on (rp.c_bpartner_id = ce.c_bpartner_id and ce.isactive = 'Y')
  left join c_doctype dt on (rp.c_doctype_id = dt.c_doctype_id)
  left join no_rol_pago_provision_line pl on (pl.no_rol_pago_provision_id = rp.no_rol_pago_provision_id and
                                              pl.no_tipo_ingreso_egreso_id in ('0AC414CB9CB64B1F85EE9DFB8BF8899C'))
  left join c_period cp on (rp.c_period_id = cp.c_period_id)
  left join no_area_empresa em on (rp.no_area_empresa_id = em.no_area_empresa_id)
  left join ad_org ad on (ad.ad_org_id = rp.ad_org_id)
 where rp.ispago = 'Y'
   and rp.c_period_id = $P{c_period_id}
order by 2,3,1]]>
	</queryString>
