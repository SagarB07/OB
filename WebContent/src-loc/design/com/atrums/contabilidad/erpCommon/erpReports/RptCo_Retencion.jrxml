	<queryString>
		<![CDATA[SELECT
     RC.ruc_bp AS RUC,
     LO.CITY || ', ' || RC.fecha_emision + 0 AS FECHA,
     CASE When RC.tipo_comprobante_venta = '01' Then 'Factura' END AS TIPO_COMP,
     RC.no_comprobante_venta AS NUM_COMP,
     RC.total_retencion AS TOTAL,
     L.address1 AS DIRECCION,
     BL.PHONE AS TELEFONO,
     c_bpartner.name AS TERCERO
FROM
CO_RETENCION_COMPRA RC,
C_BPARTNER_LOCATION BL,
C_LOCATION L,
AD_ORGINFO O,
C_LOCATION LO,
C_BPARTNER
WHERE
    RC.co_retencion_compra_id = $P{DOCUMENT_ID}
AND RC.c_bpartner_location_id = BL.c_BPARTNER_location_id
AND BL.c_location_id = L.c_location_id
AND RC.c_bpartner_id = c_bpartner.c_bpartner_id
AND RC.AD_ORG_ID = O.AD_ORG_ID
AND O.C_LOCATION_ID = LO.C_LOCATION_ID]]>
	</queryString>
