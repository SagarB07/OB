	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="27"/>
	<property name="ireport.y" value="0"/>
	<parameter name="CUENTA_BANCO" class="java.lang.String"/>
	<parameter name="FECHA_DESDE" class="java.util.Date"/>
	<parameter name="FECHA_HASTA" class="java.util.Date"/>
	<queryString>
		<![CDATA[select  ff.name, coalesce(ff.accountno,'--') as accountno,
        (select sum((ft.paymentamt*(-1)+ft.depositamt))
         from fin_finacc_transaction ft
         where ft.statementdate+0 >=
            (select min(f.statementdate)
             from fin_finacc_transaction f)
         and ft.statementdate+0 < $P{FECHA_DESDE}
and ft.fin_financial_account_id = $P{CUENTA_BANCO}) as saldo_anterior,
        (select sum((ft.paymentamt*(-1)+ft.depositamt))
            from fin_finacc_transaction ft
            where ft.statementdate+0 >=
                (select min(f.statementdate)
                from fin_finacc_transaction f)
            and ft.statementdate <= $P{FECHA_HASTA}
and ft.fin_financial_account_id = $P{CUENTA_BANCO})as saldo_actual,
        ft.statementdate, fp.documentno, coalesce(ft.description,''),
        ft.paymentamt*(-1) as paymentamt, ft.depositamt, 	   (ft.paymentamt*(-1)+ft.depositamt) as saldo_linea,
        ft.fin_financial_account_id
from fin_finacc_transaction ft,  fin_payment fp,  fin_financial_account ff
where ft.fin_payment_id = fp.fin_payment_id
and ft.fin_payment_id=fp.fin_payment_id
and ft.fin_financial_account_id = ff.fin_financial_account_id
and ff.fin_financial_account_id = $P{CUENTA_BANCO}
and ft.statementdate+0 >= $P{FECHA_DESDE}
and ft.statementdate+0 <= $P{FECHA_HASTA}
order by ft.statementdate desc]]>
	</queryString>
